<table class = "table table-hover">
  <thead>
    <tr>
      <th>ジャンル検索</th>
    </tr>
  </thead>
  <tbody>
    <% Genre.all.each do |genre| %>
    <tr>
      <td>
        <%= link_to genre.name, "/public/genres/#{genre.id}/search/", method: :get %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>