<%= form_with url: admin_search_path, method: :get, local: true do |f| %>
  <%= f.text_field :content, placeholder: "Search", class: "form-control me-2" %>
  <%= f.select :model, options_for_select({"Customer" => "customer", "Item" => "item"}) %>
  <%= f.submit "検索", class: "btn btn-secondary btn-sm ml-3" %>
<% end %>